  "private": true,
  "scripts": {
    "lint": "eslint src --ext .ts",
    "build": "tsc && cp src/conf.prod.json dist/conf.json && cp -r scripts dist/ && cp package.json dist/",
    "mk-folders": "mkdir -p logs && mkdir -p logs-simulateur",
    "nerell-simu-link": "ln -sfn ../../../java/robots/nerell-parent/nerell-simulator/logs ./logs-simulateur/nerell",
    "odin-simu-link": "ln -sfn ../../../java/robots/odin-parent/odin-simulator/logs ./logs-simulateur/odin",
    "start": "yarn mk-folders && yarn nerell-simu-link && yarn odin-simu-link && ts-node ./src/server.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "date-fns": "^2.15.0",
    "express": "^4.17.3",
    "express-ws": "^4.0.0",
    "first-line": "^1.0.3",
    "glob": "^7.1.1",
    "@influxdata/influxdb-client": "^1.33.2",
    "@influxdata/influxdb-client-apis": "^1.33.2",
    "last-line": "0.0.3",
    "lodash": "^4.17.21",
    "log4js": "^6.4.4",
    "pg": "^8.3.0",
    "queue-promise": "^2.2.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.6.2",
    "sequelize": "^5.22.5",
    "sequelize-typescript": "^1.0.0",
    "stream-json": "^1.7.0",
    "typescript-cacheable": "^2.1.3",
    "typescript-ioc": "^3.2.2"
  },
