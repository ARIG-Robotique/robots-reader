    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "nodemon": "^2.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.6.4"
  }
}
